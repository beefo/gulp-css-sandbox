{
  "name": "gulp-css-sandbox",
  "version": "0.0.1",
  "description": "A plugin for Gulp to sandbox css by prefixing rules with a selector",
  "keywords": [
    "gulpplugin"
  ],
  "author": {
    "name": "Steve Hansen",
    "email": "beefo@hotmail.com",
    "url": "https://github.com/beefo"
  },
  "repository": "beefo/gulp-css-sandbox",
  "scripts": {
    "test": "istanbul test _mocha --report html -- test/*.js --reporter spec",
    "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "dependencies": {
    "css": "^2.2.1",
    "gulp-util": "~3.0.7",
    "through2": "~3.0.0"
  },
  "devDependencies": {
    "mocha": "~5.2.0",
    "coveralls": "~3.0.2",
    "mocha-lcov-reporter": "~1.3.0",
    "istanbul": "~0.4.5",
    "event-stream": "~3.3.4",
    "should": "~2.1.0"
  },
  "engines": {
    "node": ">=0.8.0",
    "npm": ">=1.2.10"
  },
  "license": "MIT"
}
